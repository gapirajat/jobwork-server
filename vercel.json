{

  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)", "destination": "/index.js",
      "has": [
        {
          "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
          "headers": {
          "Access-Control-Allow-Origin": "*"
          }
        }
      ],
    }
]
}
